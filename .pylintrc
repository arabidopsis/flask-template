[MESSAGES CONTROL]
disable=missing-docstring,
        unused-argument,
        invalid-name,
        wrong-import-order,
        too-few-public-methods,
        use-dict-literal,
        no-else-return,
        too-many-lines,
        too-many-locals,
        import-outside-toplevel,
        broad-exception-caught,
        redefined-outer-name


[FORMAT]
max-line-length=115
